  "items": [
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "exceptions\n| summarize errorCount = count() by bin(timestamp, 1h), type\n| order by timestamp asc\n",
        "size": 0,
        "title": "Error Types and Count",
        "timeContext": {
          "durationMs": 86400000
        },
        "queryType": 0,
        "resourceType": "microsoft.insights/components",
        "visualization": "table"
      },
      "name": "query - 8"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "requests\n| summarize availability = (countif(success == true) * 100.0 / count()) by bin(timestamp, 1h)\n| order by timestamp desc\n",
        "size": 0,
        "aggregation": 3,
        "title": "Hourly Availability Percentage",
        "timeContext": {
          "durationMs": 86400000
        },
        "queryType": 0,
        "resourceType": "microsoft.insights/components",
        "visualization": "table",
        "chartSettings": {
          "xAxis": "timestamp",
          "yAxis": [
            "availability"
          ],
          "xSettings": {
            "label": "TIMESTAMP"
          },
          "ySettings": {
            "label": "AVAILABILITY"
          }
        }
      },
      "name": "query - 7"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "requests\n| where success == true\n| summarize SuccessCount = count() by bin(timestamp, 1h)\n| order by timestamp asc\n",
